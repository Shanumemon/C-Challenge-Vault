//Find the reverse of an array.
#include <stdio.h>

int main()
{
    int i, n;
    printf("Enter number of elements in array:\n");
    scanf("%d",&n);
    int arr[n],rev[n];
    printf("Enter elements of array:\n");
    for(i=0; i<n; i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0; i<n; i++)
    {
        rev[i] = arr[n-1-i];
    }
    printf("Reverse of given array:\n");
    for(i=0; i<n; i++)
    {
        printf("%d ",rev[i]);
    }
    return 0;
}

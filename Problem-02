//Remove Duplicates Items In An Array
#include <stdio.h>
int main()
{
    int i, j, k, n;
    printf("Enter size of array:\n");
    scanf("%d",&n);
    int arr[n];
    printf("Enter elements of array:\n");
    for(i=0; i<n; i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0; i<n; i++)
    {
        for(j=i+1; j<n; j++)
        {
            if(arr[i] == arr[j])
            {
                for(k=j; k<n-1; k++)
                {
                    arr[k] = arr[k+1];
                }
                n--;//reduce array size
                j--;//stay at same index
            }
        }
    }
    printf("Array after removing repeating elements:\n");
    for(i=0; i<n; i++)
    {
        printf("%d ",arr[i]);
    }
    return 0;
}

//Count the total number of duplicate elements in an array.
#include <stdio.h>

int main()
{
    int i, j, count, dup=0, n;
    printf("Enter size of array:\n");
    scanf("%d",&n);
    int arr[n], freq[n];
    printf("Enter elements of array:\n");
    for(i=0; i<n; i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0; i<n; i++)
    {
        freq[i] = -1;
    }
    for(i=0; i<n; i++)
    {
        if(freq[i] == -1)
        {
            count = 1;
            for(j=i+1; j<n; j++)
            {
                if(arr[i] == arr[j])
                {
                    count++;
                    freq[j] = 0;
                }
            }
            freq[i] = count;
            if(freq[i]>1)
            {
                dup++;
            }
        }
    }
    printf("Total number of duplicate elements in given array is %d",dup);
    return 0;
}

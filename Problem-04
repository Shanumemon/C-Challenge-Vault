//To find second largest element of array 
#include <stdio.h>
int main() 
{
    int i, n, max1, max2;
    printf("Enter number of elements in array:\n");
    scanf("%d",&n);
    int arr[n];
    printf("Enter elements of array:\n");
    for(i=0; i<n; i++)
    {
        scanf("%d",&arr[i]);
    }
    max1 = arr[0];
    max2 = -22222;//Very small number
    for(i=0; i<n; i++)
    {
        if(arr[i]>max1)
        {
            max2 = max1;
            max1 = arr[i];
        }
    }
    printf("Second largest element in given array is %d\n",max2);
    return 0;
}

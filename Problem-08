//Count frequency of each element in an array.
#include <stdio.h>

int main()
{
    int i, j, count, n;
    printf("Enter number of elements in array:\n");
    scanf("%d",&n);
    int arr[n];
    printf("Enter elements of array:\n");
    for(i=0; i<n; i++)
    {
        scanf("%d",&arr[i]);
    }
    int freq[n];
    for(i=0; i<n; i++)
    {
       freq[i] = -1;
    }
    for(i=0; i<n; i++)
    {
        if(freq[i] == -1)
        {
            count=1;
            for(j=i+1; j<n; j++)
            {
                if(arr[i] == arr[j])
                {
                    count++;
                    freq[j]=0;
                }
            }
            freq[i] = count;
        }
        
    }
    for(i=0; i<n; i++)
    {
        if(freq[i]>0)
        {
            printf("Frequency of %d in given array is %d\n",arr[i],freq[i]);
        }
    }
    return 0;
}

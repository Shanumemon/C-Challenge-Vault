//Check whether a number is even or odd using functions.
#include <stdio.h>

int iseven(int a)
{
    if(a%2 == 0)
    {
        return 1;
    }
    else
    {
        return 0;
    }
}

int main()
{
    int a;
    printf("Enter a number:\n");
    scanf("%d",&a);
    if(iseven(a))
    {
        printf("Even\n");
    }
    else
    {
        printf("Odd\n");
    }
    return 0;
}

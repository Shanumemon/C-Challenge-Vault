/*Input a string and remove all spaces without using extra libraries.*/

#include <stdio.h>
int main()
{
    int i=0,j;
    char s[50];
    printf("Enter string:\n");
    fgets(s, sizeof(s), stdin);
    while(s[i] != '\0')
    {
        if(s[i] == ' ')
        {
            j = i;
            while(s[j] != '\0')
            {
                s[j] = s[j+1];
                j++;
            }
            continue;
        }
        i++;
    }
    puts(s);
    return 0;
}

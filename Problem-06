/*Write a program to multiply two matrices*/
#include <stdio.h>

int main()
{
    int i, j, k, r1, c1, r2, c2, sum;
    printf("Enter number of rows and column in matrix-1:\n");
    scanf("%d %d",&r1,&c1);
    printf("Enter number of rows and column in matrix-2:\n");
    scanf("%d %d",&r2,&c2);
    if(r2 != c1)
    {
        printf("Invalid input\n");
        return 0;
    }
    int m1[r1][c1], m2[r2][c2], m[r1][c2];
    printf("Enter elements of matrix-1\n");
    for(i=0; i<r1; i++)
    {
        for(j=0; j<c1; j++)
        {
            scanf("%d",&m1[i][j]); 
        }
    }
    printf("Enter elements of matrix-2\n");
    for(i=0; i<r2; i++)
    {
        for(j=0; j<c2; j++)
        {
            scanf("%d",&m2[i][j]); 
        }
    }
    for(i=0; i<r1; i++)
    {
        for(j=0; j<c2; j++)
        {
            sum = 0;
            for(k=0; k<c1; k++)
            {
                sum += m1[i][k]*m2[k][j];
            }
            m[i][j] = sum;
        }
    }
    printf("Multiplication of matrix-1 and matrix-2 is:\n");
    for(i=0; i<r1; i++)
    {
        for(j=0; j<c2; j++)
        {
            printf("%d  ",m[i][j]);
        }
        printf("\n");
    }
    return 0;
}
